{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["DOM", "ESNext"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".svelte-kit"]
}
